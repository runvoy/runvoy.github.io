  "version": "1.0.0",
  "description": "GitHub Action to cleanup old deployments",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc && ncc build src/main.ts -o dist --source-map --license licenses.txt",
    "package": "npm run build",
    "lint": "eslint src/**/*.ts",
